{
  "name": "@odavl/cli",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "bin": {
    "odavl": "bin/odavl.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "prepublishOnly": "pnpm run build",
    "prepack": "pnpm run build",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc -b --noEmit",
    "test": "vitest run --coverage --reporter=default --outputFile=../../reports/phase2/cli-test.log"
  },
  "dependencies": {
    "@odavl/codemods": "workspace:*",
    "@odavl/policy": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5.9.2",
    "eslint": "^9.36.0",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.17.0"
}
