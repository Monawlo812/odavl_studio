{
  "name": "@odavl/core",
  "version": "0.4.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.17.0",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --check .",
    "typecheck": "tsc -b --noEmit",
    "build": "tsc -b",
    "test": "vitest run --coverage --reporter=default --outputFile=../../coverage/core-test.log",
    "sbom": "cyclonedx-npm --output-format json --output-file=../../reports/sbom/core-sbom.json"
  },
  "devDependencies": {
    "typescript": "^5.6.3",
    "eslint": "^9.10.0",
    "prettier": "^3.3.3",
    "vitest": "^2.1.3",
    "@types/node": "^22.7.5",
    "@cyclonedx/cyclonedx-npm": "^1.9.0"
  }
}
