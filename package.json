{
  "name": "odavl-studio",
  "version": "0.5.0",
  "private": true,
  "packageManager": "pnpm@10.17.0",
  "workspaces": [
    "apps/launcher",
    "apps/orchestrator",
    "apps/vscode-ext",
    "apps/*",
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "sbom": "cyclonedx-npm --output-format json --output-file sbom.json",
    "sbom:generate": "pnpm list --json --recursive > reports/phase3/security/sbom.cdx.json",
    "scan:osv": "pnpm dlx osv-scanner -r . > reports/phase3/security/osv.log 2>&1",
    "scan:gitleaks": "pnpm dlx gitleaks detect --no-git -s . --redact --report-format=json --report-path=reports/phase3/security/gitleaks.log",
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "echo \"no tests\" && exit 0"
  },
  "dependencies": {
    "@eslint/js": "^9.36.0",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@cyclonedx/bom": "^4.1.2",
    "@cyclonedx/cyclonedx-library": "^9.0.0",
    "@cyclonedx/cyclonedx-npm": "^4.0.3",
    "@size-limit/preset-app": "^11.2.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "@vitest/coverage-v8": "^3.2.4",
    "c8": "^10.1.3",
    "depcheck": "^1.4.7",
    "esbuild": "^0.25.10",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "gitleaks": "^1.0.0",
    "globals": "^16.4.0",
    "jest": "^30.1.3",
    "knip": "^5.64.0",
    "lockfile-lint": "^4.14.1",
    "markdownlint-cli": "^0.45.0",
    "prettier": "^3.6.2",
    "prom-client": "^15.1.3",
    "prometheus-client": "^0.1.1",
    "semgrep": "^0.0.1",
    "size-limit": "^11.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "tsup": "^8.5.0",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2",
    "vite": "^7.1.7",
    "vitest": "^3.2.4"
  }
}
