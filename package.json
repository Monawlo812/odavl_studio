{
  "name": "odavl-studio",
  "private": true,
  "packageManager": "pnpm@10.17.0",
  "workspaces": [
    "apps/*",
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "turbo run format",
    "e2e": "node scripts/e2e-golden.mjs",
    "weekly": "node scripts/weekly-report.mjs",
    "launcher": "pnpm --filter @odavl/launcher run dev"
  },
  "devDependencies": {
    "odavl-cli": "workspace:*",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  }
}